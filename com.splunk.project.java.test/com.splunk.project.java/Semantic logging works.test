Content-Type: text/ecl
Entry-Name: .content

get-menu "File/New/Project..." | click
with [get-window "New Project"] {
    get-tree | select "Splunk/Splunk SDK for Java project"
    get-button "Next >" | click
}
with [get-window -class WizardDialog] {
    get-editbox -after [get-label "Project name:"] | set-text testproject
    get-group "Add support for logging libraries" | get-button "Add logging support:" | check
    get-button Finish | click
}

with [get-view "Project Explorer" | get-tree] {
    select testproject | double-click
    select "testproject/src" | double-click
    select "testproject/src/\\(default package\\)" | double-click
    select "testproject/src/\\(default package\\)/Program.java" | click -default
    select "testproject/src/\\(default package\\)/Program.java" | double-click
}

with [get-editor "Program.java" | get-text-viewer] {
	set-text ""

	set-text "import com.splunk.logging.SplunkLogEvent;\n" +
		"public class Program {\n" +
		"\tpublic static void main(String[] args) {\n" +
		"\t\tSystem.out.println(\n" +
		"\t\t\tnew SplunkLogEvent(\"ssh:login\", \"login\") {{\n" +
		"\t\t\t\taddPair(\"boris\", \"natasha\");\n" +
		"\t\t\t}}.toString()\n" +
		"\t\t);\n" +
		"\t}\n" +
		"}"
}
get-button "Save (M1+S)" | click


get-view "Project Explorer" | get-tree | select "testproject/src/\\(default package\\)/Program.java" 
    | get-menu "Run As/1 Java Application" | click

with [get-view Console | get-text-viewer] {
    set-caret-pos 1 31
    select-range 1 30 1 79
}
with [get-view Console | get-text-viewer] {
    get-property "getSelectionText()" | equals "name=\"ssh:login\" event_id=\"login\" boris=\"natasha\"" | verify-true
}
