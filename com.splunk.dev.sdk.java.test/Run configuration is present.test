Content-Type: text/ecl
Entry-Name: .content

get-menu "File/New/Project..." | click
with [get-window "New Project"] {
    get-tree | select "Java/Java Project"
    get-button "Next >" | click
}
with [get-window "New Java Project"] {
    get-editbox -after [get-label "Project name:"] | set-text abcd
    get-button Finish | click
}
get-menu "Run/Run Configurations..." | click
get-window "Run Configurations" | get-tree | get-item "Monitored Java Application" | get-property caption 
    | equals "Monitored Java Application" | verify-true
get-window "Run Configurations" | get-tree | select "Monitored Java Application" | double-click
get-window "Run Configurations" | get-tab-folder | get-property pages -index 4 | equals Splunk | verify-true
get-window "Run Configurations" | get-tab-folder | get-tab-item Splunk | click
with [get-window "Run Configurations"] {
    get-editbox -after [get-label "Host:"] | get-property text | equals localhost | verify-true
    get-editbox -after [get-label "Port:"] | get-property text | equals 10000 | verify-true
}
