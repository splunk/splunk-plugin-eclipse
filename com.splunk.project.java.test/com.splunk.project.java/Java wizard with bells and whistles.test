Content-Type: text/ecl
Entry-Name: .content

get-menu "File/New/Project..." | click
with [get-window "New Project"] {
    get-tree | select "Splunk/Splunk SDK for Java project"
    get-button "Next >" | click
}
with [get-window -from "Window.createShell()"] {
    get-editbox -after [get-label "Project name:"] | set-text abcd
    with [get-group "Add optional Splunk data format support"] {
        get-button "Add JSON support to project (optional)" | check
        get-button "Add CSV support to project (optional)" | check
    }
    get-group "Add support for logging libraries" | get-button "Add logging support:" | check
    get-button Finish | click
}
with [get-view "Project Explorer" | get-tree] {
    get-item "abcd/src" | get-property caption | equals src | verify-true
    get-item "abcd/src" | get-property index | equals 0 | verify-true
}
with [get-view "Project Explorer" | get-tree] {
    get-item "abcd/splunk-sdk-java-1.2.1.jar" | get-property caption | equals "splunk-sdk-java-1.2.1.jar" | verify-true
    get-item "abcd/opencsv-2.3.jar" | get-property caption | equals "opencsv-2.3.jar" | verify-true
    get-item "abcd/gson-2.1.jar" | get-property caption | equals "gson-2.1.jar" | verify-true
    get-item "abcd/logback-classic-1.0.0.jar" | get-property caption | equals "logback-classic-1.0.0.jar" | verify-true
    get-item "abcd/logback-core-1.0.0.jar" | get-property caption | equals "logback-core-1.0.0.jar" | verify-true
    get-item "abcd/slf4j-api-1.6.4.jar" | get-property caption | equals "slf4j-api-1.6.4.jar" | verify-true
    get-item "abcd/splunk-library-javalogging.jar" | get-property caption | equals "splunk-library-javalogging.jar" | verify-true
}
get-view "Project Explorer" | get-tree | get-item "abcd/logback.xml" | get-property caption | equals "logback.xml" 
    | verify-true
