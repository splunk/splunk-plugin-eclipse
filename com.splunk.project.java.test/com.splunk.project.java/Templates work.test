get-menu "File/New/Project..." | click
with [get-window "New Project"] {
    get-tree | select "Splunk/Splunk SDK for Java project"
    get-button "Next >" | click
}
with [get-window -from "Window.createShell()"] {
    get-group "Add support for logging libraries" | get-button "Add logging support:" | check
    with [get-group "Add optional Splunk data format support"] {
        get-button "Add CSV support to project (optional)" | check
        get-button "Add JSON support to project (optional)" | check
    }
    get-editbox -after [get-label "Project name:"] | set-text abcd
    get-button Finish | click
}
get-view "Project Explorer" | get-tree | select abcd | get-menu "New/Class" | click
with [get-window "New Java Class"] {
    get-editbox -after [get-label "Name:"] | set-text AProgram
    with [get-button "public static void main(String[] args)"] {
        check
    }
    get-button Finish | click
}
with [get-editor "AProgram.java" | get-text-viewer] {
    set-caret-pos 9 1
    key-type Tab
    type-text splcon
    key-type "M1+SPACE"
}
get-editor "AProgram.java" | get-editbox | get-property text | equals "import com.splunk.Service;\n"
    + "import com.splunk.ServiceArgs;\n\n\n"
    + "public class AProgram {\n\n"
    + "\t/**\n"
    + "\t * @param args\n"
    + "\t */\n"
    + "\tpublic static void main(String[] args) {\n"
    + "\t\t// TODO Auto-generated method stub\n"
    + "\t\t// Create login parameters. We suggest finding\n"
    + "\t\t// a better way to store these than hard coding\n"
    + "\t\t// them in your program for production code.\n"
    + "\t\tServiceArgs serviceArgs = new ServiceArgs();\n"
    + "\t\tserviceArgs.setUsername(username);\n"
    + "\t\tserviceArgs.setPassword(password);\n"
    + "\t\tserviceArgs.setHost(host);\n"
    + "\t\tserviceArgs.setPort(port);\n\n"
    + "\t\t// Create a Service instance and log in with the argument map\n"
    + "\t\tService service = Service.connect(serviceArgs);\n"
    + "\t}\n\n"
    + "}" | verify-true
